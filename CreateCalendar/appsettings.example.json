{
  "PnPCore": {
    "DisableTelemetry": "false",
    "HttpRequests": {
      "UserAgent": "NONISV|SharePointPnP|PnPPS",
      "Timeout": "100",
      "SharePointRest": {
        "UseRetryAfterHeader": "false",
        "MaxRetries": "10",
        "DelayInSeconds": "3",
        "UseIncrementalDelay": "true"
      },
      "MicrosoftGraph": {
        "UseRetryAfterHeader": "true",
        "MaxRetries": "10",
        "DelayInSeconds": "3",
        "UseIncrementalDelay": "true"
      }
    },
    "PnPContext": {
      "GraphFirst": "true",
      "GraphCanUseBeta": "true",
      "GraphAlwaysUseBeta": "false"
    },
    "Credentials": {
      "DefaultConfiguration": "interactive",
      "Configurations": {
        "interactive": {
          "ClientId": "[your-microsoft-client-id]",
          "TenantId": "[the-tennant-id-of-sharepoint-tenant]",
          "Interactive": {
            "RedirectUri": "http://localhost"
          }
        }
      }
    },
    "Sites": {
      "SiteToWorkWith": {
        "SiteUrl": "https://[sharepoint-tennant-name].sharepoint.com/teams/[team-name]",
        "AuthenticationProviderName": "interactive"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "CreateCalendar": {
    "GoogleUser":  "[your-google-username]@gmail.com",
    "Calendars": [
      {
        "ExcelRoster": {
          "Url": "/teams/[team-name]/Shared Documents/[roster-file].xlsx",
          "DateCol": 1,
          "DateCommentsCol": 3,
          "EmployeeNamesBeneath": "Roster",
          "SpecialShiftHeaders": [ "2nd Oncall" ],
          "IgnoreShifts": ["RDO", "PH"],
          "NonAvailableShifts": [ "leave", "PDL", "?leave" ]
        },
        "FormatShift": "{0} PCCU",
        "IcsFolder": "Calendars/SCUH PCCU",
        "IcsFilename": "{0} PCCU",
        "AppointmentKeyValues": {
          "LOCATION": "SCUH",
          "ORGANIZER;CN=Jane Doe": "mailto:Jane.Doe@example.com"
        }
      },
      {
        "ExcelRoster": {
          "Url": "/teams/[other-team-name]/Shared Documents/[roster-file].xlsx",
          "DateCol": 1,
          "DateCommentsCol": 3,
          "EmployeeNamesBeneath": "Roster",
          "IgnoreShifts": ["RDO"],
          "NonAvailableShifts": [ "leave", "PDL", "SD" ]
        },
        "FormatShift": "{0} SCUH ICU",
        "IcsFolder": "Calendars/SCUH ICU",
        "IcsFilename": "{0} SCUH ICU",
        "AppointmentKeyValues": {
          "LOCATION": "SCUH",
          "ORGANIZER;CN=John Doe": "mailto:john.doe@example.com"
        }
      }
    ]
  }
}